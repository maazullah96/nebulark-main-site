{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://nebulark-main-site-server.vercel.app/api/$1",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    },
    {
      "src": "/assets/(.*)",
      "dest": "https://nebulark-main-site.vercel.app/assets/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
